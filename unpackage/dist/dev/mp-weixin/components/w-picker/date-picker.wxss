/*字体*/
.w-picker-flex2{
	-webkit-box-flex:2;
	-webkit-flex:2;
	        flex:2;
}
.w-picker-flex1{
	-webkit-box-flex:1;
	-webkit-flex:1;
	        flex:1;
}
.w-picker-view {
	width: 100%;
	height: 476rpx;
	overflow: hidden;
	background-color: rgba(255, 255, 255, 1);
	z-index: 666;
}
.d-picker-view{
	height: 100%;
}
.w-picker-item {
  text-align: center;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
}
